{
  "name": "goes-camera-monitoring-service",
  "version": "1.0.0",
  "main": "build/drivers/webserver/server.js",
  "license": "MIT",
  "scripts": {
    "ts:check": "tsc --noEmit",
    "dev": "tsx watch src/drivers/webserver/server.ts",
    "setup": "yarn install && yarn prisma generate && yarn prisma migrate dev",
    "db-ui": "yarn prisma studio",
    "start": "node build/drivers/webserver/server.js",
    "build": "tsup",
    "seed": "npx ts-node src/drivers/db/prisma/seed.ts",
    "lint": "eslint src",
    "test": "vitest run --dir 'test/unit'",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage --dir 'test/unit'",
    "sonar": "node sonar-project.js",
    "dependency:check": "dependency-check --project 'MeuProjetoNode' --scan ./ --out report",
    "dead:code": "ts-prune -i 'used in module'"
  },
  "prisma": {
    "schema": "src/drivers/db/prisma/schema.prisma"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@faker-js/faker": "8.1.0",
    "@types/luxon": "3.4.2",
    "@types/node": "20.12.12",
    "@types/nodemailer": "6.4.15",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "@vitest/coverage-istanbul": "1.3.1",
    "@vitest/coverage-v8": "1.3.1",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-local-rules": "3.0.2",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-unused-imports": "3.0.0",
    "lint-staged": "15.2.2",
    "owasp-dependency-check": "0.0.23",
    "prettier": "3.0.3",
    "prisma": "5.4.2",
    "sonarqube-scanner": "3.3.0",
    "supertest": "7.0.0",
    "ts-node": "10.9.2",
    "ts-prune": "0.10.3",
    "tsup": "7.2.0",
    "tsx": "4.16.2",
    "typescript": "5.2.2",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "1.6.0",
    "vitest-sonar-reporter": "2.0.0"
  },
  "dependencies": {
    "@azure/storage-blob": "12.24.0",
    "@fastify/cookie": "9.3.1",
    "@fastify/jwt": "8.0.0",
    "@fastify/multipart": "8.3.0",
    "@fastify/swagger": "8.11.0",
    "@fastify/swagger-ui": "1.10.1",
    "@prisma/client": "5.4.2",
    "axios": "1.6.8",
    "crypto": "1.0.1",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "dotenv": "16.3.1",
    "eslint": "8.51.0",
    "fastify": "4.24.1",
    "libphonenumber-js": "1.11.8",
    "nodemailer": "6.9.15",
    "zod": "3.22.4"
  }
}
